{
	"cli": {
		"version": ">= 0.47.0"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"android": {
				"image": "latest"
			}
		},
		"preview": {
			"distribution": "internal",
			"android": {
				"image": "latest"
			}
		},
		"production": {
			"ios": {
				"autoIncrement": "buildNumber",
				"image": "macos-monterey-12.4-xcode-13.4"
			},
			"android": {
				"image": "latest",
				"autoIncrement": "versionCode"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
