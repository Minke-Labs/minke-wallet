{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@src/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@helpers/*": ["src/helpers/*"],
			"@stores/*": ["src/stores/*"],
			"@models/*": ["src/model/*"],
			"@assets/*": ["assets/*"]
		}
	}
}
