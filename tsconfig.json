{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@src/*": ["old/src/*"],
			"@components/*": ["old/src/components/*"],
			"@helpers/*": ["old/src/helpers/*"],
			"@stores/*": ["old/src/stores/*"],
			"@models/*": ["old/src/model/*"],
			"@assets/*": ["assets/*"]
		}
	}
}
