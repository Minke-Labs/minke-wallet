{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@src/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@helpers/*": ["src/helpers/*"],
			"@stores/*": ["src/stores/*"],
			"@models/*": ["src/model/*"],
			"@assets/*": ["assets/*"],
			"@styles": ["src/styles/index"],
			"@screens": ["src/screens/index"],
			"@contexts": ["src/contexts/index"],
			"@routes": ["src/routes/index"],
			"@templates": ["src/templates/index"],
		}
	}
}
