{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"jsx": "react",
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@localization": ["src/localization/index"],
			"@components": ["src/components/index"],
			"@helpers/*": ["src/helpers/*"],
			"@stores/*": ["src/stores/*"],
			"@models/*": ["src/model/*"],
			"@assets/*": ["assets/*"],
			"@styles": ["src/styles/index"],
			"@screens": ["src/screens/index"],
			"@contexts": ["src/contexts/index"],
			"@routes": ["src/routes/index"],
			"@layouts": ["src/layouts/index"],
			"@containers": ["src/containers/index"],
			"@images": ["src/images/index"],
			"@avatars": ["src/avatars/index"],
			"@hooks": ["src/hooks/index"],
			"@utils/*": ["src/utils/*"],
			"@src/*": ["src/*"]
		}
	},
	"typeRoots": ["./src/types"]
}
